[local]
localhost ansible_connection=local

[web]
express.local ansible_connection=ssh ansible_ssh_user=root

[inventory]
inventory.local ansible_connection=ssh ansible_ssh_user=root

[logging]
logs.local ansible_connection=ssh ansible_ssh_user=root

[deploy_targets]
express.local ansible_connection=ssh ansible_ssh_user=root
logs.local ansible_connection=ssh ansible_ssh_user=root

[host_list]
express.local
inventory.local
logs.local